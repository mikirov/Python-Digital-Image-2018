                <pubDate>Sun, 20 May 2018 13:24:04 PDT</pubDate>
                        <media:title type="plain">Birabuki Ivornose auction (CLOSED)</media:title>
        <media:keywords></media:keywords>
                        <media:rating>nonadult</media:rating>
                <media:category label="Fantasy">digitalart/paintings/fantasy</media:category>
        <media:credit role="author" scheme="urn:ebu">Tokyozilla</media:credit>
